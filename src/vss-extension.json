{
    "manifestVersion": 1,
    "id": "epinova-dxp-contentsync-extension",
    "publisher": "epinova-sweden",
    "version": "0.0.10",
    "name": "Epinova DXP content synchronizer",
    "description": "Tasks to synchronize content (database/blobs) from Optimizely (formerly known as Episerver) DXP to Azure environment.",
    "public": true,
    "icons":{
        "default": "img/Epinova_e_128x128.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "tags":[
        "Release",
        "Epinova",
        "Episerver",
        "Optimizely",
        "Deploy",
        "DXC",
        "DXP",
        "Publish",
        "Install",
        "Continuous Integration",
        "Continuous Delivery",
        "Build",
        "Release",
        "Azure Pipelines",
        "Episerver Content Cloud",
        "Optimizely Content Cloud",
        "Episerver Commerce Cloud",
        "Optimizely Commerce Cloud",
        "EpiCloud"
    ],
    "branding":{
        "color": "rgb(255,255,255)",
        "theme": "light"
    },
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "license.md"
        }
    },
    "links": {
        "repository": {
            "uri": "https://github.com/Epinova/epinova-dxp-contentsynchronizer"
        },
        "company": {
            "uri": "https://www.epinova.se"
        },
        "contact": {
            "uri": "mailto:info@epinova.se"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/Epinova/epinova-dxp-contentsynchronizer"
    },
    "contributions": [
        {
            "id": "dxp-exportdb-buildtask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "ExportDb"
            }
        },
        {
            "id": "dxp-exportblobs-buildtask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "ExportBlobs"
            }
        },
        {
            "id": "dxp-contentcopy-buildtask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "ContentCopy"
            }
        },
        {
            "id": "dxp-importdxpdbtoazure-buildtask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "ImportDxpDbToAzure"
            }
        },
        {
            "id": "dxp-importdxpblobstoazure-buildtask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "ImportDxpBlobsToAzure"
            }
        }
    ],
    "files": [
        {
            "path": "ExportDb/ExportDbV2"
        },
        {
            "path": "ExportBlobs/ExportBlobsV2"
        },
        {
            "path": "ContentCopy/ContentCopyV2"
        },
        {
            "path": "ImportDxpDbToAzure/ImportDxpDbToAzureV2"
        },
        {
            "path": "ImportDxpBlobsToAzure/ImportDxpBlobsToAzureV2"
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        }
    ]
}
